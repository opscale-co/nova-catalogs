{
    "name": "opscale-co/nova-catalogs",
    "description": "A simple repository for managing data for the app",
    "keywords": [
        "laravel",
        "nova"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "homepage": "https://github.com/opscale-co/nova-catalogs",
    "license": "MIT",
    "authors": [
        {
            "name": "Opscale",
            "email": "development@opscale.co",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-readline": "*",
        "nova-kit/nova-packages-tool": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Opscale\\NovaCatalogs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Opscale\\NovaCatalogs\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Opscale\\NovaCatalogs\\NovaCatalogsToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "tightenco/duster": "^2.7"
    }
}
